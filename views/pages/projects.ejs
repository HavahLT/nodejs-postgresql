<!DOCTYPE html>
<html lang="en">
  <head>
    <% include ../partials/head %>
  </head>

  <body class="container">
    <% include ../partials/header %>
    <main>
      <div class="row">
        <div class="col-sm-8">

          <div class="jumbotron">
            <h1>Projects</h1>
              <p>List</p>
              <ul>
                  <% projects.forEach(function(projects){ %>
                  <li><%= projects.name %></li>
                <% }); %>
              </ul>

          </div>

        </div>
          <div class="col-sm-4">

            <div class="well">
              <h3>Look I'm A Sidebar!</h3>
            </div>

          </div>
      </div>
    </main>

    <footer>
      <% include ../partials/footer %>
    </footer>

  </body>
</html>
